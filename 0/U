FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

vFR #calc "((2e-3 / 60) / 360)";

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type                flowRateOutletVelocity;
        volumetricFlowRate  $vFR;
        value               uniform (0 0 0);
    }

    tank
    {
        type                noSlip;
    }

    "(front|back)"
    {
        type  wedge;
    }

//    #includeEtc "caseDicts/setConstraintTypes"
}

// ************************************************************************* //
