FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Uumg            293.15;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300.5;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      uniform $Uumg;
        value           uniform $Uumg;
    }

    tank
    {
        type            externalTemperature;

        Ta              constant $Uumg;
        h               uniform 5;
        value           $internalField;
    }

    "(front|back)"
    {
        type  wedge;
    }

//    #includeEtc "caseDicts/setConstraintTypes"
}

